{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "userInput",
  "type": "object",
  "properties": {
    "username": {
      "required": true,
      "description": "Login name for the user.",
      "type": "string"
    },
    "first_name": {
      "required": false,
      "description": "First name for the user.",
      "type": "string"
    },
    "last_name": {
      "required": false,
      "description": "Last name for the user.",
      "type": "string"
    },
    "email": {
      "required": true,
      "description": "The email address for the user.",
      "type": "string"
    }
  }
}
